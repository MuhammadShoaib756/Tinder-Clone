<div class="d-flex flex-column flex-md-row align-items-center p-3 px-md-4 bg-white border-bottom shadow-sm">
    <%= link_to root_path, class: "my-0 mr-md-auto font-weight-normal" do %>
      <%= image_tag("logo_tinder.png", height:'60', width: '160') %>
    <% end %>
      
      <% if account_signed_in? %>
        <nav class="my-2 my-md-0 mr-md-3">
        <%= link_to "Browes", browse_path, class:"p-2 text-dark"%>
        <%= link_to "My Matches", matches_path, class:"p-2 text-dark"%>
        <%= link_to "Setting", edit_account_registration_path, class:"p-2 text-dark"%>
      </nav>

      <%= link_to "Sign out", destroy_account_session_path, method: :delete, class: "btn btn-secondary p-2" %>

      <% else %>
        <nav class="my-2 my-md-0 mr-md-3">
            <a class="p-2 text-dark" href="#">Features</a>
            <a class="p-2 text-dark" href="#">Enterprise</a>
            <a class="p-2 text-dark" href="#">Support</a>
            <a class="p-2 text-dark" href="#">Pricing</a>
        </nav>
        <%= link_to "Sign up", new_account_registration_path, class:"btn btn-outline-primary mr-2"%>
        <%= link_to "Log in", new_account_session_path, class: "btn btn-secondary"%>
      <% end %>
      
      
     
</div>